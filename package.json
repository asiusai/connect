{
  "name": "asiusai",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.3.6",
  "scripts": {
    "dev": "concurrently -n api,site,connect bun:dev:*",
    "self-host": "bun --cwd api start",
    "dev:api": "bun --cwd api dev",
    "dev:site": "bun --cwd site dev",
    "dev:connect": "bun --cwd connect dev",
    "build": "concurrently -n site,connect bun:build:*",
    "build:connect": "bun --cwd connect build",
    "build:site": "bun --cwd site build",
    "tsc": "tsc --noEmit",
    "fix": "biome check --write",
    "test": "bun test api connect site --concurrent",
    "integration": "bun test integration --timeout 120000",
    "infra": "bun --cwd infra up",
    "postinstall": "find node_modules/.bun/capnp-ts@*/node_modules/capnp-ts -name '*.ts' ! -name '*.d.ts' -delete 2>/dev/null || true"
  },
  "workspaces": [
    "connect",
    "infra",
    "api",
    "site",
    "shared",
    "ssh"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "latest",
    "concurrently": "^9.2.1",
    "lefthook": "^2.0.14"
  },
  "dependencies": {
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.1",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "@types/ssh2": "^1.15.5",
    "@types/web-bluetooth": "^0.0.21",
    "jsonwebtoken": "^9.0.3",
    "playwright": "^1.57.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "ssh2": "^1.17.0",
    "tailwindcss": "4.1.18",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "typescript": "5.9.3"
  }
}
